<UserControl
             xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
             xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
             xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006" 
             xmlns:d="http://schemas.microsoft.com/expression/blend/2008" 
             xmlns:dx="http://schemas.devexpress.com/winfx/2008/xaml/core" xmlns:dxprg="http://schemas.devexpress.com/winfx/2008/xaml/propertygrid" x:Class="CPAS.Views.UC_ParaSetting" 
             mc:Ignorable="d" 
             d:DesignHeight="300" d:DesignWidth="300"
            Loaded="UserControl_Loaded">
    <Grid>
        <dx:DXTabControl >
            <dx:DXTabItem Header="SystemSetting" AllowHide="Default" ToolTip="Set system para" Height="40">
                <Grid Background="Transparent">
                    <dxprg:PropertyGridControl  Name="PropertyGrid"/>
                </Grid>
            </dx:DXTabItem>
        </dx:DXTabControl>

    </Grid>
</UserControl>
